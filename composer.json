{
    "name": "corex/corex",
    "type": "project",
    "description": "The corex Framework.",
    "keywords": [
        "framework",
        "corex"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "smarty/smarty": "~3.1"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "./config/config.php",
            "./config/register.php",
            "./config/smarty.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
